<flash-message
    level="{{ level|default('info') }}"
    message="{{ message }}"
    view-name="{{ _view.name }}"
    :options="{{ config('Flash')|json_encode }}"
    :params="{{ params|json_encode(constant('JSON_FORCE_OBJECT')) }}"
    :user-roles="{{ user.roles|default([])|json_encode }}">
</flash-message>
