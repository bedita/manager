<div class="commands-menu">
    <div class="project">
        {{ Html.link(__(''), {'_name': 'dashboard'}, {'title': __('Dashboard')})|raw }}
    </div>

    <div class="commands-module squared">
        {{ Layout.moduleLink()|raw }}
        {% if _view.request.getQuery('filter.type') is not iterable and _view.request.getQuery('filter.type') != 'objects' %}
            {% set elementClass = "has-background-module-" ~ _view.request.getQuery('filter.type') %}
        {% endif %}
        <div class="{{ elementClass }}"></div>
    </div>

    <div class="commands-buttons">
        {{ _view.fetch('commands')|raw }}
        {% if currentModule.hints.multiple_types and types.right is iterable %}
            {% element 'filter_type' %}
        {% endif %}
    </div>

    <div class="commands-links">
        {{ _view.fetch('commands-links')|raw }}
    </div>

    <div class="commands-footer">
        {{ _view.fetch('commands-footer')|raw }}
    </div>
</div>
<div></div>
