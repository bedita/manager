{% set meta = data.meta %}



<div class="tab"><h2>{{ __('Metadata') }}</h2></div>

<fieldset id="advancedproperties" class="multimediaiteminside">

<table class="bordered">
    <tbody>
        <tr>
            <th>id:</th>
            <td>{{ data.id }}</td>
        </tr>
        {% for key, val in meta %}
        <tr>
            <th>{{ key }}:</th>
            <td>{% if val is iterable %}{{ val|json_encode() }}{% else %}{{ val }}{% endif %}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>

</fieldset>
