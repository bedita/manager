<property-view
    :tab-open="tabsOpen"
    label={{ __('Publish') }}>

    <fieldset id="publish_properties">

        {% for key, value in properties.publish %}
            {% set options = Schema.controlOptions(key, value, schema.properties[key]) %}
            {{ Form.control(key, options)|raw }}
        {% endfor %}

    </fieldset>

</property-view>
